        <script>



            $(document).ready(function () {



               $('#idHocKyParentals').on('select2:select', function (e) {
                    var data = e.params.data;
                    $.ajax({
                        type: "POST",
                        url: '/PhuHuynh/CommonParentals/UpdateSelectHocKy',
                        data: { idHocKy: data.id },
                        success: function (dataResult) {
                            location.reload();
                        },
                    });
                });

                $('#idNienHocParentals').on('select2:select', function (e) {
                    var data = e.params.data;
                    $.ajax({
                        type: "POST",
                        url: '/PhuHuynh/CommonParentals/UpdateSelectNienHoc',
                        data: { idHocSinh: $('#idHocSinhParentals').val(),idNienHoc: data.id },
                        success: function (dataResult) {
                            location.reload();
                        },
                    });
                });

                $('#idLopHocParentals').on('select2:select', function (e) {
                    debugger
                    var data = e.params.data;
                    $.ajax({
                        type: "POST",
                        url: '/PhuHuynh/CommonParentals/UpdateSelectLopHoc',
                        data: { idHocSinh: $('#idHocSinhParentals').val(), idNienHoc: $('#idNienHocParentals').val(), idLopHoc: data.id },
                        dataType: "JSON",
                        success: function (dataResult) {
                            location.reload();
                        },
                    });

                });

                $('#idHocSinhParentals').on('select2:select', function (e) {
                    var data = e.params.data;
                    $.ajax({
                        type: "POST",
                        url: '/PhuHuynh/CommonParentals/UpdateSelectHocSinh',
                        data: { idHocSinh: data.id },
                        success: function (dataResult) {
                            location.reload();
                        },
                    });

                });
            });

            // Thong Bao Moi
            $(document).ready(function () {
                SoThongBaoNewHeader();
            });
            function SoThongBaoNewHeader() {
                $.ajax({
                    url: "/PhuHuynh/ThongBaoParentals/DemSoThongBaoHeader",
                    type: "GET",
                    dataType: "json",
                    success: function (data) {
                        if (data.chuaDoc > 0) {
                            $("#tbmheader").html(data.chuaDoc);
                        }
                        if (data.soTuongTac > 0) {
                            $("#ttheader").html(data.soTuongTac);
                        }
                    }
                });
            }
            function ReadThongBaoMoi() {
                $.ajax({
                    url: "/PhuHuynh/ThongBaoParentals/DemSoThongBaoNew",
                    type: "GET",
                    dataType: "json",
                    success: function (data) {
                        var model = data.chuaDocModels;
                        var url = "";
                        if (model.length > 0) {
                            $('#bodyThongBao').empty();
                            for (var i = 0; i < model.length; i++) {
                                if (model[i].EnumLoaiDoiTuongGui == 10)
                                {
                                    url = "/Content/layoutmaster/images/Truong2.png";
                                }
                                else if (model[i].EnumLoaiDoiTuongGui == 20)
                                {
                                    url = "/Content/layoutmaster/images/gv.png";
                                }
                                else if (model[i].EnumLoaiDoiTuongGui == 210)
                                {
                                    url = "/Content/layoutmaster/images/default-avatar.png";
                                }
                                var li = document.createElement('li');
                                li.innerHTML = `<a class="lineThongBao" href="#" onclick="updateDemSoTB(${model[i].ID})">
                                                <span style="float:left;height: 100%;"><img style="width: 50px;margin-right: 15px;" src="${url}"></span>
                                                <span class="details" style="display: block;overflow: hidden;max-height: 60px;"> ${model[i].TieuDe}</span>
                                                <p style="color: #138ad4;"> ${model[i].TenLoaiDoiTuongGui}<span class="time"> ${model[i].NgayDangText}</span></p>
                                                </a>`;

                                document.getElementById('bodyThongBao').appendChild(li);
                            }
                        } else {
                            $('#bodyThongBao').empty();
                            var div = document.createElement('div');
                            div.setAttribute('style', 'text-align:center;margin:10px;');
                            div.innerHTML = `
                                    <li>
                                        <span style="padding-top:8px !important;color:#003f59;">Không có thông báo mới</span>
                                    </li>
                                `;
                            document.getElementById('bodyThongBao').appendChild(div);
                        }
                    }
                });
            }
            function updateDemSoTB(e) {
                window.open('/PhuHuynh/ThongBaoParentals/DetailThongBao?id=' + e, '_blank');
                ajaxUpdateDemSoTB();
            };
            function ajaxUpdateDemSoTB() {
                $.ajax({
                    url: "/PhuHuynh/ThongBaoParentals/DemSoThongBaoNew?demSo=1",
                    type: "GET",
                    dataType: "json",
                    success: function (data) {
                        if (data.chuaDoc > 0) {
                            $('#bodyThongBao').empty();
                            $("#tbmheader").html(data.chuaDoc);
                        } else {
                            $("#tbmheader").empty();
                            $("#tbMoi").empty();
                        }
                    }
                });
            }


            //Tuong Tac
            
            function GetTuongTac() {
                $.ajax({
                    url: "/PhuHuynh/ThongBaoParentals/DemSoTuongTac",
                    type: "GET",
                    dataType: "json",
                    success: function (data) {
                        var model = data.danhSachTB;
                        if (model.length > 0) {
                            $('#bodyTuongTac').empty();
                            for (var i = 0; i < model.length; i++) {
                                if (model[i].EnumLoaiDoiTuongGui == 10)
                                {
                                    url = "/Content/layoutmaster/images/Truong2.png";
                                }
                                else if (model[i].EnumLoaiDoiTuongGui == 20)
                                {
                                    url = "/Content/layoutmaster/images/gv.png";
                                }
                                else if (model[i].EnumLoaiDoiTuongGui == 210)
                                {
                                    url = "/Content/layoutmaster/images/default-avatar.png";
                                }
                                var contentTT = document.createElement('li');
                                contentTT.innerHTML = `<a class="lineThongBao" href="#" onclick="updateDemSoTuongTac(${model[i].IdTB})">
                                                        <span style="float:left;height: 100%;"><img style="width: 50px;margin-right: 15px;" src="${url}"></span>
                                                        <span class="details" style="display: block;overflow: hidden;max-height: 60px;"> ${model[i].TieuDeText}</span>
                                                        <p style="color: #138ad4;"> ${model[i].EnumLoaiDoiTuongGuiText}</p>
                                                        </a>`;

                                document.getElementById('bodyTuongTac').appendChild(contentTT);
                            }
                        } else {
                            $('#bodyTuongTac').empty();
                            var divEmpty = document.createElement('div');
                            divEmpty.setAttribute('style', 'text-align:center;margin:10px;');
                            divEmpty.innerHTML = `
                                    <li>
                                        <span style="padding-top:8px !important;color:#003f59;">Không có bình luận hoặc lượt thích mới</span>
                                    </li>
                                `;
                            document.getElementById('bodyTuongTac').appendChild(divEmpty);
                        }
                    }
                });
            }
            function updateDemSoTuongTac(e) {
                if (e === 1) {
                    ajaxUpdateDemSoTuongTac(1);
                } else if (e === 0) {
                    ajaxUpdateDemSoTuongTac(0);
                } else {
                    window.open('/PhuHuynh/ThongBaoParentals/DetailThongBao?id=' + e, '_blank');
                    ajaxUpdateDemSoTuongTac();
                }

            };
            function ajaxUpdateDemSoTuongTac(e) {
                $.ajax({
                    url: "/PhuHuynh/ThongBaoParentals/DemSoTuongTac" +"?isReadAll=" + e,
                    type: "GET",
                    dataType: "json",
                    success: function (data) {
                        if (data.soTB > 0) {
                            $('#bodyTuongTac').empty();
                            $("#ttheader").html(data.soTB);
                        }else {
                            $("#bodyTuongTac").empty();
                            $("#ttheader").empty();
                        }
                    }
                });
            }

        </script>

